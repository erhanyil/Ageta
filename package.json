{
  "name": "ageta",
  "version": "1.0.0",
  "description": "Ageta",
  "main": "main.js",
  "productName": "Ageta",
  "scripts": {
    "start": "electron main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Ageta --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon1.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Ageta\"",
    "package-linux": "electron-packager . Ageta --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [],
  "author": "Ageta",
  "license": "CC0-1.0",
  "devDependencies": {
    "bower": "^1.7.9",
    "electron": "^1.8.4",
    "electron-packager": "^12.0.0",
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "dotenv": "^2.0.0",
    "md5": "^2.1.0",
    "node-notifier": "^5.2.1"
  }
}